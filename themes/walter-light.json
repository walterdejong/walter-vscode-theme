{
	"$schema": "vscode://schemas/color-theme",
	"name": "Walter in the Light",
	"colors": {
		"editor.background": "#f8f8f8",
		"editor.foreground": "#000",
		"editorIndentGuide.background": "#ccc",
		"editorIndentGuide.activeBackground": "#aaa",
		"editorWhitespace.foreground": "#f8f8f8",
		"activityBarBadge.background": "#007ACC",
		"sideBarTitle.foreground": "#444",
		"sideBar.background": "#fff",
		"sideBar.foreground": "#444",
		"list.dropBackground": "#383B3D",
		"list.hoverBackground": "#ddd",
		"input.placeholderForeground": "#444",
		"input.foreground": "#000",
		"settings.textInputBorder": "#CECECE",
		"settings.numberInputBorder": "#292929",
		"menu.background": "#eee",
		"menu.foreground": "#000",
		"statusBarItem.remoteForeground": "#FFF",
		"statusBarItem.remoteBackground": "#16825D",
		"editorCursor.background": "#000",
		"editorCursor.foreground": "#00e000",
		"editorRuler.foreground": "#aaa",
		"editor.selectionBackground": "#c0e0ee",
		"editor.selectionHighlightBackground": "#eee",
		"editor.inactiveSelectionBackground": "#eee",
		"editor.findMatchBackground": "#c0e0ee",
		"editor.findMatchHighlightBackground": "#ddd"
	},
	"tokenColors": [
		{
			"scope": [
				"meta.embedded",
				"source.groovy.embedded"
			],
			"settings": {
				"foreground": "#000"
			}
		},
		{
			"scope": "emphasis",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "strong",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": "header",
			"settings": {
				"foreground": "#000"
			}
		},
		{
			"scope": "comment",
			"settings": {
				"foreground": "#070"
			}
		},
		{
			"scope": "constant.language",
			"settings": {
				"foreground": "#800"
			}
		},
		{
			"scope": [
				"constant.numeric"
			],
			"settings": {
				"foreground": "#070"
			}
		},
		{
			"scope": "constant.regexp",
			"settings": {
				"foreground": "#04a"
			}
		},
		{
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#04a"
			}
		},
		{
			"scope": "entity.name.tag.css",
			"settings": {
				"foreground": "#800"
			}
		},
		{
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#800"
			}
		},
		{
			"scope": [
				"entity.other.attribute-name.class.css",
				"entity.other.attribute-name.class.mixin.css",
				"entity.other.attribute-name.id.css",
				"entity.other.attribute-name.parent-selector.css",
				"entity.other.attribute-name.pseudo-class.css",
				"entity.other.attribute-name.pseudo-element.css",
				"source.css.less entity.other.attribute-name.id",
				"entity.other.attribute-name.attribute.scss",
				"entity.other.attribute-name.scss"
			],
			"settings": {
				"foreground": "#b90"
			}
		},
		{
			"scope": "invalid",
			"settings": {
				"background": "#c00",
				"foreground": "#fff"
			}
		},
		{
			"scope": "markup.underline",
			"settings": {
				"foreground": "#04a"
			}
		},
		{
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#000"
			}
		},
		{
			"scope": "markup.heading",
			"settings": {
				"foreground": "#800"
			}
		},
		{
			"scope": "markup.italic",
			"settings": {
				"foreground": "#d60"
			}
		},
		{
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#070"
			}
		},
		{
			"scope": "markup.deleted",
			"settings": {
				"background": "#c00",
				"foreground": "#fff"
			}
		},
		{
			"scope": "markup.changed",
			"settings": {
				"foreground": "#04a"
			}
		},
		{
			"scope": "punctuation.definition.quote.begin.markdown",
			"settings": {
				"foreground": "#800"
			}
		},
		{
			"scope": "punctuation.definition.list.begin.markdown",
			"settings": {
				"foreground": "#800"
			}
		},
		{
			"scope": "markup.inline.raw",
			"settings": {
				"foreground": "#070"
			}
		},
		{
			"scope": "markup.raw.block.markdown",
			"settings": {
				"foreground": "#070"
			}
		},
		{
			"name": "brackets of XML/HTML tags",
			"scope": "punctuation.definition.tag",
			"settings": {
				"foreground": "#666"
			}
		},
		{
			"scope": "meta.preprocessor",
			"settings": {
				"foreground": "#fff"
			}
		},
		{
			"scope": "meta.preprocessor.string",
			"settings": {
				"foreground": "#04a"
			}
		},
		{
			"scope": "meta.preprocessor.numeric",
			"settings": {
				"foreground": "#000"
			}
		},
		{
			"scope": "meta.structure.dictionary.key.python",
			"settings": {
				"foreground": "#04a"
			}
		},
		{
			"scope": "meta.diff.header",
			"settings": {
				"foreground": "#04a"
			}
		},
		{
			"scope": "storage",
			"settings": {
				"foreground": "#800"
			}
		},
		{
			"scope": "storage.type",
			"settings": {
				"foreground": "#800"
			}
		},
		{
			"scope": "storage.modifier",
			"settings": {
				"foreground": "#800"
			}
		},
		{
			"scope": "string",
			"settings": {
				"foreground": "#04a"
			}
		},
		{
			"scope": "string.tag",
			"settings": {
				"foreground": "#04a"
			}
		},
		{
			"scope": "string.value",
			"settings": {
				"foreground": "#04a"
			}
		},
		{
			"scope": "string.regexp",
			"settings": {
				"foreground": "#04a"
			}
		},
		{
			"scope": "string.quoted.docstring.multi.python",
			"settings": {
				"foreground": "#070"
			}
		},
		{
			"name": "String interpolation",
			"scope": [
				"punctuation.definition.template-expression.begin",
				"punctuation.definition.template-expression.end",
				"punctuation.section.embedded"
			],
			"settings": {
				"foreground": "#04a"
			}
		},
		{
			"name": "Reset JavaScript string interpolation expression",
			"scope": [
				"meta.template.expression"
			],
			"settings": {
				"foreground": "#666"
			}
		},
		{
			"scope": [
				"support.type.vendored.property-name",
				"support.type.property-name",
				"variable.css",
				"variable.scss",
				"variable.other.less",
				"source.coffee.embedded"
			],
			"settings": {
				"foreground": "#666"
			}
		},
		{
			"scope": "keyword",
			"settings": {
				"foreground": "#800"
			}
		},
		{
			"scope": "keyword.control",
			"settings": {
				"foreground": "#800"
			}
		},
		{
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#800"
			}
		},
		{
			"scope": [
				"keyword.operator.new",
				"keyword.operator.expression",
				"keyword.operator.cast",
				"keyword.operator.sizeof",
				"keyword.operator.instanceof",
				"keyword.operator.logical.python"
			],
			"settings": {
				"foreground": "#800"
			}
		},
		{
			"scope": "keyword.other.unit",
			"settings": {
				"foreground": "#666"
			}
		},
		{
			"scope": "support.type",
			"settings": {
				"foreground": "#800"
			}
		},
		{
			"scope": [
				"punctuation.section.embedded.begin.php",
				"punctuation.section.embedded.end.php"
			],
			"settings": {
				"foreground": "#04a"
			}
		},
		{
			"scope": "support.function.git-rebase",
			"settings": {
				"foreground": "#04a"
			}
		},
		{
			"scope": "constant.sha.git-rebase",
			"settings": {
				"foreground": "#b90"
			}
		},
		{
			"name": "coloring of the Java import and package identifiers",
			"scope": [
				"storage.modifier.import.java",
				"variable.language.wildcard.java",
				"storage.modifier.package.java"
			],
			"settings": {
				"foreground": "#666"
			}
		},
		{
			"name": "this.self",
			"scope": "variable.language",
			"settings": {
				"foreground": "#800"
			}
		},
		{
			"scope": "entity.name",
			"settings": {
				"foreground": "#000"
			}
		},
		{
			"scope": "keyword.control.directive",
			"settings": {
				"foreground": "#800"
			}
		},
		{
			"scope": "variable",
			"settings": {
				"foreground": "#000"
			}
		},
		{
			"scope": "entity.name.function",
			"settings": {
				"foreground": "#000"
			}
		},
		{
			"scope": "entity.name.function.operator",
			"settings": {
				"foreground": "#000"
			}
		},
		{
			"scope": "entity.name.function.preprocessor",
			"settings": {
				"foreground": "#000"
			}
		},
		{
			"scope": "entity.name.label",
			"settings": {
				"foreground": "#000"
			}
		},
		{
			"scope": "entity.name.namespace",
			"settings": {
				"foreground": "#000"
			}
		},
		{
			"scope": [
				"entity.name.type",
				"entity.name.class"
			],
			"settings": {
				"foreground": "#000"
			}
		},
		{
			"scope": "entity.name.operator.custom-literal",
			"settings": {
				"foreground": "#070"
			}
		},
		{
			"scope": "entity.name.operator.custom-literal.number",
			"settings": {
				"foreground": "#070"
			}
		},
		{
			"scope": "entity.name.operator.custom-literal.string",
			"settings": {
				"foreground": "#04a"
			}
		},
		{
			"scope": [
				"keyword.operator.new",
				"keyword.operator.delete"
			],
			"settings": {
				"foreground": "#800"
			}
		},
		{
			"scope": "variable.other.enummember",
			"settings": {
				"foreground": "#000"
			}
		},
		{
			"scope": "variable.other.property",
			"settings": {
				"foreground": "#000"
			}
		},
		{
			"scope": "variable.parameter",
			"settings": {
				"foreground": "#000"
			}
		}
	]
}
